<div class="container">

    <div class="container">
        <h3>Vagas cadastrar para o usuario <%=current_headhunter.email%></h3>
        <p><%= link_to 'Cadastrar vaga', new_job_vacancy_path %>
    </div>

    <div class="container">
        <%if @job_vacancies.empty?%>
            <p>Seu perfil nÃ£o possui vagas cadastradas</p>
        <%else%>
            <% @job_vacancies.each do |vacancy|%>
                <%=link_to vacancy.title, vacancy%>
            <%end%>
        <%end%>
    </div>




</div>